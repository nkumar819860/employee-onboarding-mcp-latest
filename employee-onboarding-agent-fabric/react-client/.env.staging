# Staging Environment Configuration
# Employee Onboarding MCP System - CloudHub Staging

# Main Agent Broker (CloudHub Staging)
REACT_APP_API_BASE_URL=http://agent-broker-mcp-server.us-e1.cloudhub.io

# Individual MCP Services (CloudHub Staging)
REACT_APP_EMPLOYEE_API_URL=http://employee-onboarding-mcp-server.us-e1.cloudhub.io
REACT_APP_ASSET_API_URL=http://asset-allocation-mcp-server.us-e1.cloudhub.io
REACT_APP_NOTIFICATION_API_URL=http://notification-mcp-server.us-e1.cloudhub.io

# API Configuration
REACT_APP_API_TIMEOUT=12000
REACT_APP_RETRY_ATTEMPTS=3

# Application Settings
REACT_APP_ENV=staging
REACT_APP_DEBUG=false
REACT_APP_MOCK_DATA_FALLBACK=true

# CloudHub Region
REACT_APP_CLOUDHUB_REGION=us-east-1

# Staging Features
REACT_APP_ENABLE_PERFORMANCE_MONITORING=true
REACT_APP_ENABLE_ERROR_TRACKING=true
